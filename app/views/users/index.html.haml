%h1 Users all

- if @users.any?
  %table
    %thead
      %th
        Name
      %th
        Image
    %tbody
      - @users.each do |user|
        %tr
          %td
            = user.name
          %td
            = image_tag user.avatar.variant(resize: "50x50")
- else
  %p Users Not found.

= link_to "New user", new_user_path